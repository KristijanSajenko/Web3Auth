    "loglevel": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^14",
    "@types/webpack-env": "1.16.3",
    "css-loader": "^2.1.1",
    "file-loader": "^6.2.0",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.5",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "@web3auth/customauth-adapter": "^0.1.1"
    
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "files": [
