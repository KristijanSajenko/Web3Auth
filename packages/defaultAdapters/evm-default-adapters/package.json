{
    "name": "@web3auth/evm-default-adapters",
    "version": "0.1.1",
    "description": "default evm chain compatible adapters for web3auth modal",
    "keywords": [
      "web3Auth/evm-default-adapters",
      "web3Auth",
      "multichainWallet",
      "blockchain",
      "ethereum",
      "solana"
    ],
    "main": "dist/evmDefaultAdapters.cjs.js",
    "module": "dist/evmDefaultAdapters.esm.js",
    "unpkg": "dist/evmDefaultAdapters.umd.min.js",
    "jsdelivr": "dist/evmDefaultAdapters.umd.min.js",
    "types": "dist/types/index.d.ts",
    "author": "Torus Labs",
    "homepage": "https://github.com/Web3Auth/Web3Auth/tree/main/packages/defaultAdapters/evm-default-adapters#readme",
    "license": "ISC",
    "scripts": {
      "test": "mocha --config ../../.mocharc.json test/**.ts",
      "test-debugger": "mocha --config ../../.mocharc.json --inspect-brk test/**.ts",
      "dev": "rimraf dist/ && torus-scripts start",
      "build": "rimraf dist/ && torus-scripts build",
      "lint": "eslint --fix 'src/**/*.ts'",
      "prepack": "yarn run build",
      "pre-commit": "lint-staged --cwd ."
    },
    "files": [
      "dist",
      "src"
    ],
    "devDependencies": {
      "lint-staged": "^12.1.7",
      "@toruslabs/openlogin": "^1.3.4",
      "@types/lodash.clonedeep": "^4.5.6"
    },
    "dependencies": {
      "@web3auth/base": "^0.3.0",
      "@web3auth/metamask-adapter": "^0.3.0",
      "@web3auth/openlogin-evm-adapter": "^0.3.1",
      "@web3auth/torus-evm-adapter": "^0.3.0",
      "@web3auth/wallet-connect-v1-adapter": "^0.3.0",
      "lodash.clonedeep": "^4.5.0",
      "loglevel": "^1.8.0"
    },
    "lint-staged": {
      "!(*d).ts": [
        "eslint --cache --fix",
        "prettier --write"
      ]
    },
    "publishConfig": {
      "access": "public"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/Web3Auth/Web3Auth.git"
    },
    "bugs": {
      "url": "https://github.com/Web3Auth/Web3Auth/issues"
    }
  }
  